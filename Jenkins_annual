pipeline {
agent any
tools {maven "MAVEN_HOME"}
stages {
    stage('checkout')
    {
	steps{
      checkout scm
	  }
    }
    stage('test run')
    {
	steps{
      bat 'mvn clean install'
	  }
    }
    stage('Sonar Qube anaysis')
    {
	steps{
	withSonarQubeEnv('Test_Sonar') {bat 'mvn sonar:sonar'}
	} 
    }
  }
}
